{
  "name": "nabzkala",
  "version": "0.0.0",
  "description": "The Nabzkala e-commerce platform.",
  "scripts": {
    "build": "turbo run build --filter=swr-toolkit",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
